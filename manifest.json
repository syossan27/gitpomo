{
  "name": "gitpomo",
  "version": "1",
  "manifest_version": 2,
  "description": "Manage pomodoro for github issue",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "https://github.com/*/*/issues",
    "https://github.com/*/*/issues/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
